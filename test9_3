""" import pandas as pd
df = pd.DataFrame([["a", "b", "c"], ["d", "e", "f"], ["g", "h", "i"]])
print(df)
print("\n---------------\n")
 """

import pandas as pd
data={
    'x': [10,15,20],
    'y' : [50,55,60],
    'z' : [100,110,120]
}

idx=['x축','y축','z축']
fr=pd.DataFrame(data,index=idx)

print(fr['x'])
print(fr['z'])


frs=pd.DataFrame(fr,columns=['x','y','z','t'])
print(frs)
print(\n====================================\n')